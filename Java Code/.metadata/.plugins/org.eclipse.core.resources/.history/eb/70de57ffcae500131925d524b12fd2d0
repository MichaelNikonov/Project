package SGIEntities;

import java.util.Date;

public class IImageSearchLog {

	private String _location, _date, _time;
	private Client _client;
	private Date _datetime;
	private boolean _result;
	
	public IImageSearchLog(String loc, String dt, String tm, Client clnt, 
			Date dtstamp, boolean res) {
		_location = loc;
		_date = dt;
		_time = tm;
		_client = clnt;
		_datetime = dtstamp;
		_result = res;
	}
	
	public String getLocation() { return _location; }
	public String getDate() { return _date; }
	public String getTime() { return _time; }
	public Client getClient() { return _client; }
	public Date getDateTime() { return _datetime; }
	public boolean getResult() { return _result; }

}
